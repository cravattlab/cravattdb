{
  "name": "cravattdb",
  "version": "1.0.0",
  "description": "A user-facing database for chemoproteomics data.",
  "repository": "cravattlab/cravattdb",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run build:dll && npm run build:dev",
    "build:dll": "webpack --config config/webpack.dll.js --progress --profile --bail",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --bail",
    "dev": "npm run build && webpack-dev-server --config config/webpack.dev.js --inline --hot",
    "dev:only": "webpack-dev-server --config config/webpack.dev.js --inline --hot",
    "clean": "npm run clean:dist && npm run clean:tsc",
    "clean:dist": "rimraf dist",
    "clean:tsc": "rimraf app/**/*.js && rimraf app/**/*.map",
    "postinstall": "npm run build"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "chroma-js": "1.2.1",
    "core-js": "2.4.1",
    "jquery": "3.1.0",
    "lodash": "4.16.1",
    "rxjs": "5.0.0-beta.12",
    "semantic-ui-css": "2.2.4",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "@angularclass/hmr": "1.2.1",
    "@angularclass/hmr-loader": "3.0.2",
    "@types/chroma-js": "1.1.32",
    "@types/core-js": "0.9.34",
    "@types/lodash": "4.14.36",
    "@types/node": "6.0.40",
    "add-asset-html-webpack-plugin": "1.0.2",
    "angular2-template-loader": "0.5.0",
    "awesome-typescript-loader": "2.2.4",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "happypack": "2.2.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "typescript": "2.0.2",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "2.1.0-beta.5",
    "webpack-merge": "0.14.1"
  }
}
